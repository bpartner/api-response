{
	"name": "bpartner/api-response",
	"description": "Create response by simple way for Laravel",
	"type": "library",
	"license": "MIT",
	"require": {
        "php": "^8.2",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
	"autoload": {
		"psr-4": {
			"Bpartner\\ApiResponse\\": "src/",
			"Bpartner\\ApiResponse\\Tests\\": "tests/",
			"Bpartner\\ApiResponse\\Database\\Factories\\": "database/factories/",
			"Bpartner\\ApiResponse\\Database\\Seeders\\": "database/seeders/"
		}
	},
	"minimum-stability": "stable",
	"extra": {
		"laravel": {
			"providers": [
				"Bpartner\\ApiResponse\\Providers\\ApiResponseServiceProvider"
			],
            "aliases": {
                "API": "Bpartner\\ApiResponse\\API"
            }
		}
	}
}
